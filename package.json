{
  "name": "yax-router",
  "version": "0.0.1",
  "description": "Router plugin for yax (Using react-router)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "eslint src test --fix",
    "build:lib": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:lib && npm run build:es",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test nyc mocha --no-timeouts",
    "prepublish": "npm test",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/yax-router.git"
  },
  "keywords": [
    "yax",
    "router",
    "react-router",
    "react-router-redux"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/yax-router/issues"
  },
  "homepage": "https://github.com/d-band/yax-router#readme",
  "peerDependencies": {
    "react-router": "*",
    "yax": "*"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "react-router-redux": "^5.0.0-alpha.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.0",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^22.0.3",
    "mocha": "^5.0.0",
    "nyc": "^11.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-test-renderer": "^16.0.0",
    "yax": "^0.3.0"
  }
}
